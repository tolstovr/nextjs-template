    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "^5"
